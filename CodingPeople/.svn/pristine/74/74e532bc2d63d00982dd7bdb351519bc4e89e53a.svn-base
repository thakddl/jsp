/* ****************** */
/* 마우스오버point */
/* ****************** */
.mouseover {
	margin-top: -32px;
	margin-left: 85px;
	top: -2px;
	right: -25px;
	text-align: center;
	background-color: #BCDBEA;
	border-radius: 50%;
	width: 24px;
	height: 24px;
	font-size: 14px;
	line-height: 26px;
	cursor: default;
}

.mouseover:before {
	content: '?';
	font-weight: bold;
	color: #fff;
}

.mouseover:hover p {
	display: block;
	transform-origin: 100% 0%;
	-webkit-animation: fadeIn 0.3s ease-in-out;
	animation: fadeIn 0.3s ease-in-out;
}

.mouseover p {
	display: none;
	text-align: left;
	background-color: #1E2021;
	padding: 20px;
	width: 300px;
	position: absolute;
	border-radius: 3px;
	box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.2);
	left: 80px;
	color: #FFF;
	font-size: 13px;
	line-height: 1.4;
}

.mouseover p:before {
	position: absolute;
	content: '';
	width: 0;
	height: 0;
	border: 6px solid transparent;
	border-bottom-color: #1E2021;
	left: 10px;
	top: -12px;
}

.mouseover p:after {
	width: 100%;
	height: 40px;
	content: '';
	position: absolute;
	top: -40px;
	left: 0;
}

