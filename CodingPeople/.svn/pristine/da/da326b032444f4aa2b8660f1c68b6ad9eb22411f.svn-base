package coin.vo;

/**
 * Member테이블 전체 칼럼 가지고오는 VO
 * - 전체 회원정보 조회 시 활용
 * @author lhb
 *
 */
public class MemberVO {
	private String memId; 	// 회원아이디
	private String memPass;	// 회원 비밀번호
	private String mgId;	// 회원등급
	private String memName;	// 회원명
	private String memBir;	// 회원생일(19900101)
	private String memAdd1;	// 주소1 (회원가입 시 우편번호 검색기능으로 입력받음)
	private String memAdd2;	// 주소2 (상세 주소)
	private String memZip;	// 우편번호
	private String memTel;	// 회원 전화번호
	private String memMail;	// 회원 이메일주소
	private String memCoin;	// 코딩코인
	private String memPoint;// 활동포인트
	private String msId;	// 회원 상태 (가입상태)
	private String memDate;	// 가입날짜
	
	public String getMemId() {
		return memId;
	}
	public void setMemId(String memId) {
		this.memId = memId;
	}
	public String getMemPass() {
		return memPass;
	}
	public void setMemPass(String memPass) {
		this.memPass = memPass;
	}
	public String getMgId() {
		return mgId;
	}
	public void setMgId(String mgId) {
		this.mgId = mgId;
	}
	public String getMemName() {
		return memName;
	}
	public void setMemName(String memName) {
		this.memName = memName;
	}
	public String getMemBir() {
		return memBir;
	}
	public void setMemBir(String memBir) {
		this.memBir = memBir;
	}
	public String getMemAdd1() {
		return memAdd1;
	}
	public void setMemAdd1(String memAdd1) {
		this.memAdd1 = memAdd1;
	}
	public String getMemAdd2() {
		return memAdd2;
	}
	public void setMemAdd2(String memAdd2) {
		this.memAdd2 = memAdd2;
	}
	public String getMemZip() {
		return memZip;
	}
	public void setMemZip(String memZip) {
		this.memZip = memZip;
	}
	public String getMemTel() {
		return memTel;
	}
	public void setMemTel(String memTel) {
		this.memTel = memTel;
	}
	public String getMemMail() {
		return memMail;
	}
	public void setMemMail(String memMail) {
		this.memMail = memMail;
	}
	public String getMemCoin() {
		return memCoin;
	}
	public void setMemCoin(String memCoin) {
		this.memCoin = memCoin;
	}
	public String getMemPoint() {
		return memPoint;
	}
	public void setMemPoint(String memPoint) {
		this.memPoint = memPoint;
	}
	public String getMsId() {
		return msId;
	}
	public void setMsId(String msId) {
		this.msId = msId;
	}
	public String getMemDate() {
		return memDate;
	}
	public void setMemDate(String memDate) {
		this.memDate = memDate;
	}
	
}
