* {
	margin: 0;
	/* 마진 리셋 */
	padding: 0;
	/* 패딩 리셋 */
	box-sizing: border-box;
	/* 박스 영역은 테두리까지 */
}

a {
	text-decoration: none;
	/* 텍스트 링크 밑줄 없애기 */
}

#wrap-container {
	margin: 0 auto;
	/* 화면 중앙에 배치 */
	width: 1400px;
	/* 너비 */
	margin: auto;
	background-color: #fff;
}
/*********************************/
/* 헤더 영역 - 로고와 내비게이션 */
/*********************************/
header {
	margin-top: 20px;
	/* 헤더 위 띄우기 */
	width: 100%;
	/*  너비 */
	height: 100px;
	/* 높이 */
	background-color: #045;
}
/* 로고 */
#logo {
	float: left;
	/* 왼쪽으로 플로팅 */
	width: 250px;
	/*  너비 */
	height: 100px;
	/* 높이 */
	line-height: 50px;
	/* 세로로 중간에 맞춤 - 줄간격을 높이 값과 같게 */
	padding-left: 20px;
	/* 왼쪽에 여백 */
}

#logo h1 {
	font-family: 'Merienda', cursive;
	font-weight: 700;
	font-size: 40px;
	/* 글자 크기 */
	color: #fff;
	/* 글자 색*/
	text-shadow: 0 -1px 0 #222;
	/* 그림자 색 */
}
/* 내비게이션 */
.nav1 {
	float: right;
	/* 오른쪽으로 플로팅 */
	width: 300px;
	/*   너비 */
	padding-top: 0px;
	/* 메뉴를 아래로 내리기 위해 */
}

#topMenu {
	list-style: none;
}

#topMenu>li {
	float: right;
	/* 오른쪽으로 플로팅 */
	position: static;
}

#topMenu>li>a {
	display: block;
	/* 링크 텍스트는 블록으로 */
	font-size: 1.0em;
	color: #fff;
	/* 글자 색 */
	font-weight: 600;
	/* 글자 굵기 */
	padding: 10px;
	/* 패딩 */
}

#topMenu>li>p {
	display: block;
	/* 링크 텍스트는 블록으로 */
	font-size: 1.0em;
	color: #fff;
	/* 글자 색 */
	font-weight: 600;
	/* 글자 굵기 */
	padding: 10px;
	/* 패딩 */
}

#topMenu>li>a:hover {
	color: #1fa8f8;
	text-shadow: 0 -2px #222;
}

.nav2 {
	float: left;
	width: 800px;
	padding-top: 45px;
	padding-left: 100px;
}

#BottomMenu {
	list-style: none;
}

#bottomMenu>li {
	float: left;
	/* 메뉴 항목을 오른쪽으로 플로팅 */
	position: relative;
}

#bottomMenu>li>a {
	display: block;
	/* 링크 텍스트는 블록으로 */
	font-size: 1.4em;
	color: rgb(255, 255, 255);
	/* 글자 색 */
	font-weight: 600;
	/* 글자 굵기 */
	padding: 20px 30px;
	/* 패딩 */
}

#bottomMenuli {
	font-size: 1.4em;
	color: rgb(255, 255, 255);
	/* 글자 색 */
	font-weight: 600;
	/* 글자 굵기 */
	padding: 20px 30px;
	/* 패딩 */
}

#bottomMenu>li>a:hover {
	color: #f9fd13;
	text-shadow: 0 -2px #222;
}
/*********************************/
/* 컨테이너 영역 */
/*********************************/
#container {
	margin: 0 auto;
	/* 화면 중앙에 배치 */
	width: 1400px;
	/* 너비 */
	margin: auto;
	background-color: #fff;
}

.container {
	margin-bottom: 15px;
}

.container::after {
	content: "";
	display: block;
	clear: both;
}
